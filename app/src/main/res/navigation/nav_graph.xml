<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.lenschanger.ui.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_openingFragment2"
            app:destination="@id/openingFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_mainFragment_to_lensesFragment"
            app:destination="@id/lensesFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_mainFragment_to_infLensFragment22"
            app:destination="@id/infLensFragment2"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_mainFragment_to_tuPutOnFragment22"
            app:destination="@id/tuPutOnFragment2"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_mainFragment_to_infOpeningFragment22"
            app:destination="@id/infOpeningFragment2"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_mainFragment_to_settingFragment22"
            app:destination="@id/settingFragment2"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/openingFragment"
        android:name="com.example.lenschanger.ui.opening.OpeningFragment"
        android:label="fragment_opening"
        tools:layout="@layout/fragment_opening" >
        <action
            android:id="@+id/action_openingFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/lensesFragment"
        android:name="com.example.lenschanger.ui.lenses.LensesFragment"
        android:label="fragment_lenses"
        tools:layout="@layout/fragment_lenses"
        app:enterAnim="@android:anim/slide_out_right"
        app:exitAnim="@android:anim/slide_out_right"
        app:popEnterAnim="@android:anim/slide_in_left"
        app:popExitAnim="@android:anim/slide_out_right"
        app:popUpToInclusive="true">
        <action
            android:id="@+id/action_lensesFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/infLensFragment2"
        android:name="com.example.lenschanger.ui.fragment_main.InfLensFragment"
        android:label="fragment_inf_lens"
        tools:layout="@layout/fragment_inf_lens" >
        <action
            android:id="@+id/action_infLensFragment2_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/settingFragment2"
        android:name="com.example.lenschanger.ui.fragment_main.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment2_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/infOpeningFragment2"
        android:name="com.example.lenschanger.ui.fragment_main.InfOpeningFragment"
        android:label="fragment_inf_opening"
        tools:layout="@layout/fragment_inf_opening" >
        <action
            android:id="@+id/action_infOpeningFragment2_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/tuPutOnFragment2"
        android:name="com.example.lenschanger.ui.fragment_main.TuPutOnFragment"
        android:label="fragment_tu_put_on"
        tools:layout="@layout/fragment_tu_put_on" >
        <action
            android:id="@+id/action_tuPutOnFragment2_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpToInclusive="true"/>
    </fragment>
</navigation>